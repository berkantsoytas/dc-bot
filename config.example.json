{
  "logging": {
    "level": "trace",
    "filename": "bot.log",
    "quiet": false,
    "overwrite": true,
    "use_color": true,
    "http": {
      "enable": true,
      "filename": "http.log"
    },
    "disable_modules": ["WEBSOCKETS", "USER_AGENT"]
  },
  "discord": {
    "token": "<Your Discord Bot Token>",
    "default_prefix": {
      "enable": false,
      "prefix": "<Your Default Prefix>"
    }
  }
}
